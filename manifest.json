{
  "manifest_version": 3,
  "name": "Acesso Rápido - Anota AI",
  "version": "1.3",
  "description": "Extensão para acessar rapidamente links úteis na Anota AI",
  "permissions": ["activeTab", "storage", "webRequest"],
  "host_permissions": ["https://root.anota.ai/*", "https://console.tuna.uy/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}